{
  "name": "screendown",
  "displayName": "Screendown",
  "description": "Screendown is an easy-to-use tool that allows you to create beautiful screenshots of your Markdown within Visual Studio Code editor content.",
  "icon": "assets/icon-128x128.png",
  "publisher": "eliostruyf",
  "preview": true,
  "version": "0.0.19",
  "repository": {
    "url": "https://github.com/estruyf/screendown",
    "type": "git"
  },
  "galleryBanner": {
    "color": "#0e131f",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "markdown",
    "screenshot",
    "preview"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "screendown.openWebview",
        "title": "Screendown: Take a screenshot"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "screendown.openWebview",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ext": "webpack --mode development --watch --config ./webpack/extension.config.js",
    "watch:wv": "webpack serve --mode development --config ./webpack/webview.config.js",
    "package": "npm run clean && npm-run-all --parallel package:*",
    "package:ext": "webpack --mode production --config ./webpack/extension.config.js",
    "package:wv": "webpack --mode production --config ./webpack/webview.config.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "clean": "rimraf dist",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@estruyf/vscode": "^1.1.0-beta.3196622",
    "@headlessui/react": "^1.7.11",
    "@heroicons/react": "^2.0.16",
    "@tailwindcss/forms": "^0.5.3",
    "@types/dom-to-image": "^2.6.4",
    "@types/glob": "^7.2.0",
    "@types/image-type": "^4.0.1",
    "@types/lodash.merge": "^4.6.7",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "autoprefixer": "^10.4.13",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "image-type": "^5.2.0",
    "jsonc-parser": "^3.2.0",
    "lodash.merge": "^4.6.2",
    "mocha": "^10.0.0",
    "modern-screenshot": "^4.3.5",
    "node-fetch": "^2.6.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "recoil": "^0.7.6",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "rimraf": "^4.1.2",
    "shiki": "^0.14.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.6",
    "ts-loader": "^9.4.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
